
$ServerList = @"
RCHPWVASEWEB92A
RCHPWVASEWEB92B
"@ -split "`r`n" 

$PFXFileName = 'apply.chryslercapital.com'

$mypwd = Get-Credential -UserName 'Enter password below' -Message 'Enter password below'

foreach ($Server in $ServerList) {
    "Copying to $Server"
   Copy-Item "\\Rchpwvasemgm01\c$\Users\mp.ssummitt\Desktop\Certs\$PFXFileName.pfx" -Destination "\\$Server\C$\ASE\$PFXFileName.pfx"
}
 
foreach ($Server in $ServerList) { "$PFXFileName -->  $(Test-path "\\$Server\C$\ASE\$PFXFileName.pfx")" }
Invoke-Command -ComputerName $ServerList { Import-PfxCertificate -FilePath "C:\ASE\$($Using:PFXFileName).pfx" -CertStoreLocation Cert:\LocalMachine\My -Password $Using:mypwd.Password }
